{
  "name": "purescript-playwright",
  "version": "0.0.1",
  "description": "PureScript bindings for Playwright",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "playwright": "^1.45.1"
  },
  "scripts": {
    "test": "npx spago@next test",
    "format": "npx prettier --write src/**/*.js test/**/*.js; npx purs-tidy format-in-place src/**/*.purs test/**/*.purs",
    "check": "npx prettier --check src/**/*.js test/**/*.js; npx purs-tidy check src/**/*.purs test/**/*.purs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klntsky/purescript-playwright.git"
  },
  "keywords": [
    "purescript",
    "playwright"
  ],
  "author": "Vladimir Kalnitsky <klntsky@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/klntsky/purescript-playwright/issues"
  },
  "homepage": "https://github.com/klntsky/purescript-playwright#readme"
}
